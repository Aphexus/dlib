{
    "description": "D language utility library",
    "authors": [
        "Timur Gafarov"
    ],
    "license": "Boost",
    "homepage": "http://github.com/gecko0307/dlib",
    "name": "dlib",
    "importPaths": [
        "."
    ],
    "buildRequirements":[
        "allowWarnings"
    ],
    "configurations": [
        {
            "name": "library",
            "targetType": "library",
            "sourcePaths": ["dlib"],
            "excludedSourceFiles": [
                "dlib/math/matrix2x2.d",
                "dlib/math/matrix3x3.d",
                "dlib/math/matrix4x4.d",
                "dlib/math/affine_old.d",
                "dlib/math/matrix_old.d"
            ]
        },
        {
            "name": "import",
            "targetType": "sourceLibrary",
            "sourceFiles-posix": ["libdlib.a"],
            "sourceFiles-windows": ["dlib.lib"]
        }
    ]
}
